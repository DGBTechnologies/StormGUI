[ 
	{
		"selector": "node",
		"css": {
			"label": "data(label)",
			"height": 59,
			"width": 48,
			"shape": "rectangle",
			"background-color":"white",
			"background-fit": "cover"
		}
	},
	{
		"selector": "edge",
		"css": {
			"curve-style": "bezier",
			"control-point-step-size": 80,
			"target-arrow-shape": "triangle",
			"width": 2,
			"line-color": "#42adf5",
			"target-arrow-color": "black"
		}
	},
	{
		"selector": "edge.triggerEdge",
		"css": {
			"line-style":"dashed"
		}
	},
	{
		"selector": "node.be",
		"css": {
			"height": 42,
			"width": 42,
			"background-image": "../assets/images/be.png"
		}
	},
	{
		"selector": "node.and",
		"css": {
			"height": 66.65,
			"width": 50,
			"background-image": "../assets/images/and.png"
		}
	},
	{
		"selector": "node.or",
		"css": {
			"height": 66.65,
			"width": 50,
			"background-image": "../assets/images/or.png"
		}
	},
	{
		"selector": "node.vot",
		"css": {
			"height": 66.65,
			"width": 50,
			"background-image": "../assets/images/vot.png"
		}
	},
	{
		"selector": "node.pand",
		"css": {
			"height": 66.65,
			"width": 50,
			"background-image": "../assets/images/pand.png"
		}
	},
	{
		"selector": "node.por",
		"css": {
			"height": 66.65,
			"width": 50,
			"background-image": "../assets/images/por.png"
		}
	},
	{
		"selector": "node.pdep",
		"css": {
			"height": 50,
			"width": 100,
			"background-image": "../assets/images/pdep.png"
		}
	},
	{
		"selector": "node.fdep",
		"css": {
			"height": 50,
			"width": 100,
			"background-image": "../assets/images/fdep.png"
		}
	},
	{
		"selector": "node.spare",
		"css": {
			"height": 50,
			"width": 100,
			"background-image": "../assets/images/spare.png"
		}
	},
	{
		"selector": "node.seq",
		"css": {
			"height": 50,
			"width": 100,
			"background-image": "../assets/images/seq.png"
		}
	},
	{
		"selector": "node.block",
		"css": {
			"height": 66.65,
			"width": 50,
			"background-image": "../assets/images/dft_block.png"
		}
	},
	{
		"selector": ".eh-handle",
		"style": {
			"background-color": "red",
			"width": 12,
			"height": 12,
			"shape": "ellipse",
			"overlay-opacity": 0,
			"border-width": 12,
			"border-opacity": 0
		}
	},
	{
		"selector": "node.topLevel",
		"css": {
			"font-weight": "bold",
			"font-size":"25"
		}

	},
	{
		"selector": "node.containEvent",
		"css": {
			"color": "red"
		}

	},
	{
		"selector": "node.avoidEvent",
		"css": {
			"color": "#228B22"
		}

	},
	{
		"selector": ".eh-hover",
		"style": {
			"background-color": "red"
		}
	},
	{
		"selector": ".eh-source",
		"style": {
			"border-width": 2,
			"border-color": "red"
		}
	},
	{
		"selector": ".eh-target",
		"style": {
			"border-width": 2,
			"border-color": "red"
		}
	},
	{
		"selector": ".eh-preview, .eh-ghost-edge",
		"style": {
			"line-color": "red",
			"target-arrow-color": "red",
			"source-arrow-color": "red"
		}
	},
	{
		"selector": ".eh-ghost-edge.eh-preview-active",
		"style": {
			"opacity": 0
		}
	}
]